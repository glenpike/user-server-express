{
  "extends": "airbnb-base",
  "plugins": ["import"],
  "rules": {
    "import/prefer-default-export": "off",
    "no-var": "error",
    "prefer-const": "warn"
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
